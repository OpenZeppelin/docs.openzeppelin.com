{
  "name": "openzeppelin-docs-site-theme",
  "private": true,
  "version": "0.0.0",
  "description": "The default theme for the openzeppelin documentation site",
  "homepage": "https://docs.openzeppelin.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/openzeppelin/docs-site-theme.git"
  },
  "scripts": {
    "build-preview": "node preview/build.js",
    "dev": "fgbg 'live-server preview/build' 'nodemon -w theme -w preview preview/build.js' 'rollup -cw'",
    "bundle": "rollup -c && node bundle.js"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "adm-zip": "^0.5.5",
    "asciidoctor": "^2.2.4",
    "fgbg": "^0.1.5",
    "fromentries": "^1.3.2",
    "glob": "^7.1.7",
    "handlebars": "^4.7.7",
    "highlight.js": "^10.7.3",
    "highlightjs-solidity": "^1.1.0",
    "jszip": "^3.7.0",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.2",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.34.1",
    "sirv-cli": "^1.0.12",
    "yaml": "^1.10.2"
  }
}
